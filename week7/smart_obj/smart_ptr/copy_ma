.SUFFIXES: .cpp .h .o

OBJECTS= object.o smart_obj.o shared_mut.o test.o unique_immut.o
CPP = object.cpp smart_obj.cpp shared_mut.cpp test.cpp unique_immut.cpp

CXX = g++
CPPFLAGES = -c

TARGET = test

$(TARGET) : $(CPP) 
	$(CXX) -c $(CPP)

clean :
	rm -rf $(OBJECTS)

object.o : object.cpp

smart_obj.o : smart_obj.cpp

shared_mut.o : shared_mut.cpp

test.o : test.cpp

unique_immut.o : unique_immut.cpp
